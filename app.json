{
	"expo": {
		"name": "DoneWithIt",
		"slug": "DoneWithIt",
		"platforms": ["ios", "android", "web"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./app/assets/icon.png",
		"splash": {
			"image": "./app/assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#e63c4b"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.codebyron.donewithit",
			"buildNumber": "1.0.0"
		},
		"android": {
			"package": "com.codewithmosh.donewithit",
			"version": 1
		},
		"description": "A marketplace to sell the stuff you don't use anymore.",
		"notification": {
			"iosDisplayInForeground": true
		},
		"extra": {
			"bugsnag": {
				"apiKey": "cb22f715cab95ca134998c9f6375d8ab"
			}
		},
		"hooks": {
			"postPublish": [
				{
					"file": "@bugsnag/expo/hooks/post-publish.js",
					"config": {}
				}
			]
		}
	}
}
